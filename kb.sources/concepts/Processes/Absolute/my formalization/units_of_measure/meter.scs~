meter
	=> nrel_main_idtf:[meter](*<-lang_en;;*);
			[метр](*<-lang_ru;;*);
	<- rrel_key_sc_element:...
		(*
			<- sc_definition;;
			<= nrel_sc_text_translation:...
			(*
				-> rrel_example:
					[<b>Метр</b> - это единица измерения расстояния.]
					(*
						<- lang_ru;;
						=> nrel_format: format_html;;
					*);;
			*);;
		*);
	<= nrel_inclusion: units_of_measure;
	<= nrel_using_constants:
		{
			unit_of_measure;
                        distance
		};;

distance
       => nrel_main_idtf: [distance](*<- lang_en;;*);
                                [расстояние](*<- lang_ru;;*);;
