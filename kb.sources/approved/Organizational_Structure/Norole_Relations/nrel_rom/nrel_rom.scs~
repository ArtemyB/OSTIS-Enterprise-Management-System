nrel_rom
	=> nrel_main_idtf:[The number of flash storage](*<-lang_en;;*);
		[Количество флеш памяти](*<-lang_ru;;*);
	=> nrel_idtf:
		[ROM](*<-lang_en;;*);
		[Read-only memory](*<-lang_en;;*);
		[ПЗУ](*<-lang_ru;;*);
		[ Постоянное запоминающее устройство](*<-lang_ru;;*);
	<- rrel_key_sc_element:...
		(*
			<- sc_definition;;
			<= nrel_sc_text_translation:...
			(*
				-> rrel_example:
					"file://nrel_rom_definition_en.html"
					(*<- lang_en;;*);
					"file://nrel_rom_definition_ru.html"
					(*<- lang_ru;;*);;
			*);;
		*);
	<= nrel_inclusion: electronics; technology;;