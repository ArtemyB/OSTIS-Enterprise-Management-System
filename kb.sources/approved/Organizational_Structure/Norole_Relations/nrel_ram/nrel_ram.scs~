nrel_ram
	=> nrel_main_idtf:[Amount of RAM](*<-lang_en;;*);
		[Количество оперативной памяти](*<-lang_ru;;*);
	=> nrel_idtf:
		[ОЗУ](*<-lang_ru;;*);
	<- rrel_key_sc_element:...
		(*
			<- sc_definition;;
			<= nrel_sc_text_translation:...
			(*
				-> rrel_example:
					"file://nrel_ram_definition_en.html"
					(*<- lang_en;;*);
					"file://nrel_ram_definition_ru.html"
					(*<- lang_ru;;*);;
			*);;
		*);
	<= nrel_inclusion: electronics; technology;;