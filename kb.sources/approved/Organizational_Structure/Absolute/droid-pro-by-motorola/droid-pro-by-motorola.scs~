droid_pro_by_motorola
	=> nrel_main_idtf:[DROID Pro by Motorola](*<-lang_en;;*);
		[DROID Pro by Motorola](*<-lang_ru;;*);
	<- rrel_key_sc_element:...
		(*
			<- sc_definition;;
			<= nrel_sc_text_translation:...
			(*
				-> rrel_example:
					"file://droid_pro_by_motorola_definition_en.html"
					(*<- lang_en;;*);;
			*);;
		*);
	=> nrel_ram: 512_mb;
	=> nrel_camera: 5_mp;
	=> nrel_rom: 2_gb;
	<= nrel_inclusion: motorola; mobile_phones;;
